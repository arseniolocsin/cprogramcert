{
    CUST_REC_p_t    rec     =3D VEC_begin( vec );
    CUST_REC_p_t    vend    =3D VEC_end( vec );

    printCustomerReportHeadings( title );
    while ( rec &lt; vend )
        printCustomerReportDetail( rec++ );
}
